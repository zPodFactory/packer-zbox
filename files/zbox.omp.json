{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "<#c44569>█</><#ffffff>■</> {{ .Path }} ",
          "foreground": "#ffffff",
          "background": "#c44569",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#f78fb3",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#f78fb3",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#c4a1a1",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FCA17D{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#ffffff",
          "background": "#86BBD8",
          "type": "session",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "command": "hostname -f"
          },
          "foreground": "#ffffff",
          "background": "#33658A",
          "type": "command",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#c44569>█▓░</> ",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        },
	{
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{.Code}}{{ end }}",
          "foreground": "#F16363",
          "type": "exit",
          "style": "plain"
        },
	{
		"template": "❱",
		"type": "text",
		"style": "plain"
	}
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
